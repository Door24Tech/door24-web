{
  "hosting": {
    "site": "door-24-website",
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/404.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}

